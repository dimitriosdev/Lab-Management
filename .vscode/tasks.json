{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-wpf",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/LabManagementApp.UI.WPF/LabManagementApp.UI.WPF.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "run-api",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/LabManagementApp.API/LabManagementApp.API.csproj"
      ],
      "problemMatcher": []
    }
  ]
}
